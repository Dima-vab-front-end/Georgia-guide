{
  "name": "georgia-guide",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "entry-port": "cross-env HOST=0.0.0.0 PORT=1234",
    "serve": "npm run entry-port vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-dev": "vue-cli-service build --mode development",
    "format": "npm run prettier && npm run lint",
    "lint": "npm run lint:ts -- --fix && npm run lint:vue -- --fix",
    "lint:ts": "eslint src --cache --ext \".ts\" --max-warnings 450",
    "lint:vue": "eslint src --cache --ext \".vue\" --max-warnings 450",
    "prettier": "npm run --silent prettier:css && npm run --silent prettier:html && npm run --silent prettier:ts && npm run --silent prettier:json && npm run --silent prettier:vue",
    "prettier:css": "prettier --config .prettierrc  --check --write \"src/**/*.scss\"",
    "prettier:html": "prettier --config .prettierrc  --check --write \"src/**/*.html\"",
    "prettier:json": "prettier --config .prettierrc  --check --write \"**/*.json\"",
    "prettier:ts": "prettier --config .prettierrc  --check --write \"src/**/*.ts\"",
    "prettier:vue": "prettier --config .prettierrc  --check --write \"src/**/*.vue\"",
    "serve-prod": "npm run copy-app-config-prod && npm run entry-port vue-cli-service serve",
    "serve-prod:mac": "npm run copy-app-config-prod-mac && npm run entry-port vue-cli-service serve --mode production"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@microsoft/applicationinsights-core-js": "^3.3.4",
    "@microsoft/applicationinsights-web": "^3.3.4",
    "@papb/json-excel": "^2.0.2",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vueuse/head": "^2.0.0",
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.3",
    "bootstrap": "^5.3.7",
    "camelcase-keys": "^9.0.0",
    "core-js": "^3.32.1",
    "es6-promise": "^4.2.8",
    "guid-typescript": "^1.0.9",
    "http-status-codes": "^2.2.0",
    "keycode-js": "^3.1.0",
    "sanitize-html": "^2.11.0",
    "uuid": "^11.0.3",
    "vanilla-jsoneditor": "^2.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-popper": "^1.5.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "lint-staged": "^14.0.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.3",
    "sass": "1.69.7",
    "sass-loader": "^16.0.3",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.7.2",
    "vue-style-loader": "^4.1.3"
  }
}
